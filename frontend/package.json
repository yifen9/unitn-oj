{
  "name": "unitn-oj-frontend",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "pnpm build && pnpm db:apply:local && pnpm dev:api",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "biome format --write .",
    "lint": "biome check .",
    "test": "vitest --run",
    "db:apply:local": "wrangler d1 migrations apply DB --local",
    "db:execute:local": "wrangler d1 execute DB --local",
    "dev:api": "wrangler pages dev .svelte-kit/cloudflare --local --port 8080 --ip 0.0.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.2",
    "@cloudflare/workers-types": "^4.20250831.0",
    "@sveltejs/adapter-static": "^3.0.0",
    "@sveltejs/kit": "^2.22.0",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "@types/node": "^24.3.0",
    "sass-embedded": "^1.92.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^7.0.4",
    "vitest": "^3.2.4",
    "wrangler": "^4.33.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "packageManager": "pnpm@8.15.5"
}